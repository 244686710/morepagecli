{
  "name": "morepage",
  "version": "2.3.3",
  "description": "这是一个vue的多页打包配置",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "dev": "webpack --progress --hide-modules --env.name dev && webpack-dev-server --env.name dev  --open ",
    "build": "webpack --progress --hide-modules --env.name build",
    "dev-uat": "webpack --progress --hide-modules --env.name dev --env.api uat  && webpack-dev-server --env.name dev --env.api uat --open ",
    "build-uat": "webpack --progress --hide-modules --env.name build --env.api uat ",
    "dev-local": "webpack --progress --hide-modules --env.name dev --env.api local  && webpack-dev-server --env.name dev --env.api local  --open ",
    "build-local": "webpack --progress --hide-modules --env.name build --env.api local "

  },
  "author": "",
  "license": "ISC",
