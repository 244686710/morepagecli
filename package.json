    "vue-hot-reload-api": "^2.0.11",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^11.3.3",
    "vue-style-loader": "^2.0.4",
    "vue-template-compiler": "^2.2.5",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2"
  },
