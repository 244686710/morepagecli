    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-latest": "^6.24.0",
    "babel-runtime": "^6.23.0",
    "compression-webpack-plugin": "^0.3.2",
    "cross-env": "^4.0.0",
    "css-loader": "^0.27.3",
    "extract-text-webpack-plugin": "^2.1.0",
